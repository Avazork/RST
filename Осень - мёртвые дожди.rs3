    <segment id="1" parent="31" relname="span">Осень – </segment>
    <segment id="6" parent="1" relname="appeal">мертвые дожди?</segment>
    <segment id="2" parent="33" relname="span">
Осень – </segment>
    <segment id="7" parent="2" relname="appeal">юные морозы?</segment>
    <segment id="3" parent="5" relname="preparation">
Задубевшие березы</segment>
    <segment id="5" parent="34" relname="span">
Ковыляют по Руси.</segment>
    <segment id="4" parent="43" relname="span">
Осень – падшая листва,</segment>
    <segment id="8" parent="4" relname="elaboration">
Оплевавшая дорогу,</segment>
    <segment id="9" parent="10" relname="preparation">
Осень – смертная тревога</segment>
    <segment id="10" parent="44" relname="span">
У хмельного старика.</segment>
    <segment id="11" parent="37" relname="contrast">
А я татарин на лицо</segment>
    <segment id="12" parent="37" relname="contrast">
Да с фамилией хохляцкой,</segment>
    <segment id="13" parent="40" relname="span">
Отчего ж в тоске кабацкой</segment>
    <segment id="14" parent="13" relname="appeal">
Угодил под колесо?</segment>
    <segment id="15" parent="39" relname="conjunction">
А я зарезан без ножа,</segment>
    <segment id="16" parent="39" relname="conjunction">
Я прострелен, но не пулей.</segment>
    <segment id="17" parent="18" relname="appeal">
Вы мою свечу задули,</segment>
    <segment id="18" parent="38" relname="span">
Осень – темная душа.</segment>
    <segment id="19" parent="53" relname="span">
С неба рвет хмельная муть,</segment>
    <segment id="20" parent="47" relname="span">
Отчего Ему я каюсь?</segment>
    <segment id="21" parent="48" relname="span">
Скоро, чувствую, отмаюсь –</segment>
    <segment id="22" parent="21" relname="evidence">
Вместо крови в жилах ртуть.</segment>
    <segment id="23" parent="50" relname="span">
Поэтичность языка,</segment>
    <segment id="24" parent="23" relname="elaboration">
Легковесная химера,</segment>
    <segment id="25" parent="26" relname="preparation">
Этой дряни нету дела</segment>
    <segment id="26" parent="49" relname="span">
До осеннего меня.</segment>
    <segment id="27" parent="35" relname="conjunction">
Осень – мертвые дожди,</segment>
    <segment id="29" parent="35" relname="conjunction">
